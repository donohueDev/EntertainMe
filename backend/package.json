{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for Entertainment Reviews App",
  "main": "dist/server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-db": "ts-node src/db/testConnection.ts",
    "prisma:generate": "dotenv -e ../.env -- prisma generate",
    "prisma:migrate": "dotenv -e ../.env -- prisma migrate dev",
    "prisma:validate": "dotenv -e ../.env -- prisma validate",
    "postinstall": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@types/axios": "^0.9.36",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongodb": "^4.0.6",
    "@types/pg": "^8.15.2",
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.8.4",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
